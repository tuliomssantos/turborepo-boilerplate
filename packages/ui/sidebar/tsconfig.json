{
  "extends": "tsconfig/tsconfig.react.json",
  "include": ["src", "assets/**/*.ts", "styles.d.ts", "svg.d.ts"],
  "compilerOptions": {
    "lib": ["DOM", "DOM.Iterable", "ESNext"],
    "target": "es5",
    "moduleResolution": "node",
    "jsx": "react-jsx",
    "plugins": [{ "name": "typescript-plugin-css-modules" }]
  }
}
